{
  "name": "ergebnis/php-cs-fixer-config",
  "description": "Provides a configuration factory and rule set factories for friendsofphp/php-cs-fixer.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com",
      "homepage": "https://localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/php-cs-fixer-config",
  "support": {
    "issues": "https://github.com/ergebnis/php-cs-fixer-config/issues",
    "source": "https://github.com/ergebnis/php-cs-fixer-config",
    "security": "https://github.com/ergebnis/php-cs-fixer-config/blob/main/.github/SECURITY.md"
  },
  "require": {
    "php": "~7.4.0 || ~8.0.0 || ~8.1.0 || ~8.2.0 || ~8.3.0",
    "ext-filter": "*",
    "erickskrauch/php-cs-fixer-custom-fixers": "~1.2.1",
    "friendsofphp/php-cs-fixer": "~3.41.1",
    "kubawerlos/php-cs-fixer-custom-fixers": "~3.17.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.28.3",
    "ergebnis/data-provider": "^3.2.0",
    "ergebnis/license": "^2.4.0",
    "ergebnis/phpunit-slow-test-detector": "^2.8.0",
    "ergebnis/rector-rules": "~0.3.0",
    "fakerphp/faker": "^1.23.0",
    "infection/infection": "~0.26.6",
    "phpunit/phpunit": "^9.6.15",
    "psalm/plugin-phpunit": "~0.18.4",
    "rector/rector": "~0.18.13",
    "symfony/filesystem": "^5.0.0 || ^6.0.0",
    "symfony/process": "^5.0.0 || ^6.0.0",
    "vimeo/psalm": "^5.18.0"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\PhpCsFixer\\Config\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\PhpCsFixer\\Config\\Test\\": "test"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true
    },
    "audit": {
      "abandoned": "report"
    },
    "platform": {
      "php": "7.4.33"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  }
}
